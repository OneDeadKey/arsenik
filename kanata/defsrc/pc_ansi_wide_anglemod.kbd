;; wide-angle-mod:
;; - left shift becomes Z
;; - the right hand is moved one key to the right

(defsrc
  1    2    3    4    5  bspc   7    8    9    0    -
  q    w    e    r    t         u    i    o    p    [
  a    s    d    f    g         j    k    l    ;    '
  lsft z    x    c    v    b    m    ,    .    /    rsft
            lalt          spc             ralt
)

