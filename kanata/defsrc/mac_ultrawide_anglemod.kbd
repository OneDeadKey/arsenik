;; ultrawide-angle-mod:
;; - the ISO key (a.k.a. LSGT or 102 key) becomes Z
;; - the right hand is moved one key to the right
;; - the left hand is moved one key to the left

(defsrc
  grv    1    2    3    4    bspc   7    8    9    0    -
  tab    q    w    e    r           u    i    o    p    [
  caps   a    s    d    f           j    k    l    ;    '
  lsft   <    z    x    c      b    m    ,    .    /    rsft
                   lmet       spc        rmet
)

